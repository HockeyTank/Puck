<%- model_class = Rink -%>
<div class="page-header">
  <div class="row">
    <div class="span11"><h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1></div>
    <div class="span2"><%= link_to t('.new', :default => " <i class='icon-plus-sign'></i> Add Rink".html_safe),
            new_rink_path,
            :class => 'btn btn-primary' %>
    </div>
  </div>
</div>


<table class="table table-striped" id="rinks-table" data-source="/rinks.json">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:name) %></th>
      <th><%= model_class.human_attribute_name(:address1) %></th>
      <th><%= model_class.human_attribute_name(:addresscity) %></th>
      <th><%= model_class.human_attribute_name(:addressstate) %></th>
      <th><%= model_class.human_attribute_name(:addresszip) %></th>
      <th><%= model_class.human_attribute_name(:phone1) %></th>
      <th><%= model_class.human_attribute_name(:website) %></th>
      <th><%= model_class.human_attribute_name(:updated_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
 
  </tbody>
</table>

